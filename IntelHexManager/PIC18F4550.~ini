[SECTIONS]
Description=Bootloader Upgrade feature
Count=7

[SECTION.0]
SectionName=Vectors
RangeMin=$0000
RangeMax=$1FFF

[SECTION.1]
SectionName=VASCO ApplicationData
RangeMin=$2000
RangeMax=$20FF

[SECTION.2]
SectionName=Relocated Data0
RangeMin=$2100
RangeMax=$37FF

[SECTION.3]
SectionName=Relocation code
RangeMin=$3800
RangeMax=$3EFF

[SECTION.4]
SectionName=Relocation Records
RangeMin=$3F00
RangeMax=$3FFF


[SECTION.5]
SectionName=Relocated Data1
RangeMin=$4000
RangeMax=$5FFF

[SECTION.6]
SectionName=Relocated Data2 (32K only)
RangeMin=$6000
RangeMax=$7FFF

[SECTION.7]
SectionName=Config
RangeMin=$300000
RangeMax=$30000D

[SECTION.8]
SectionName=ID
RangeMin=$3FFFFE
RangeMax=$3FFFFF

[SECTION.8]
SectionName=EEPROM
RangeMin=$F00000
RangeMax=$F000FF

